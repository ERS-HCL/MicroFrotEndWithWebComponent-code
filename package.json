{
  "name": "football-club",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "live-server dist/football-club",
    "serve": "ng serve",
    "build": "npm run build:football-club && npm run build:microApps",
    "build:git": "ng build --prod --base-href 'https://sathyaleo08.github.io/MicroApp/'",
    "bundle": "node custom-build-script.js",
    "build:microApps": "npm run build:team && npm run build:players && npm run build:news",
    "build:team": "ng build --prod --project team --single-bundle true --output-hashing none --vendor-chunk false --output-path dist/football-club/team",
    "build:players": "ng build --prod --project players --single-bundle true --output-hashing none --vendor-chunk false --output-path dist/football-club/players",
    "build:news": "ng build --prod --project news --single-bundle true --output-hashing none --vendor-chunk false --output-path dist/football-club/news",
    "build:football-club": "ng build --project football-club",
    "build:microApps:webpack:old-solution": "webpack",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.7",
    "@angular/common": "~9.1.7",
    "@angular/compiler": "~9.1.7",
    "@angular/core": "~9.1.7",
    "@angular/elements": "^9.1.9",
    "@angular/forms": "~9.1.7",
    "@angular/platform-browser": "~9.1.7",
    "@angular/platform-browser-dynamic": "~9.1.7",
    "@angular/router": "~9.1.7",
    "@webcomponents/custom-elements": "^1.4.1",
    "bootstrap": "^4.5.0",
    "concat": "^1.0.3",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.1.0",
    "@angular-devkit/build-angular": "~0.901.6",
    "@angular/cli": "~9.1.6",
    "@angular/compiler-cli": "~9.1.7",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "live-server": "^1.2.0",
    "ngx-build-plus": "^9.0.6",
    "protractor": "~5.4.3",
    "terser": "3.14.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "webpack-cli": "^2.0.14"
  }
}
